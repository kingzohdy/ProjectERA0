;零纪元公会礼包

proc 1
	strvar ret
	GET_PACK_SPACE 0 ret
	if ret >= 7
		DEC_ITEM_NUM_INPAK 5052976 1 10
		if 10 = 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5052818 1 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5052860 1 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5213538 1 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5041030 3 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5052811 10 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5200030 10 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5200040 10 1 0
		endif
	else
		add_sys_msg_start
		add_sys_msg 背包空间不足，请预留7个空格再开启道具！
		set_sys_msg_mode 4 0 0
		msg_info
	endif
endproc
