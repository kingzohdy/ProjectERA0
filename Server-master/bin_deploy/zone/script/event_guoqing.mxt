PROC 2
    call 100
	call 101
	call 102
	add_sys_msg_start
	add_sys_msg 国庆活动已经圆满结束！
	set_sys_msg_mode 6 0 0
	svr_msg_info
ENDPROC

;10秒触发一次
PROC 3	
	GET_EVENT_VAR 0 32 1 20
	if @20 = 36
		call 200
		SET_EVENT_VAR 0 32 0 1
	else
		add 20 1
		SET_EVENT_VAR 0 32 @20 1
	endif
	
	GET_NOW_TIME 0 0 0 0 30 0 0
	if @30 >= 9 or @30 < 2 
	;国庆期间 早上九点到晚上两点才刷可疑的人
		;一分钟
		GET_EVENT_VAR 4 32 1 21
		if @21 = 6
			call 201
			call 202
			SET_EVENT_VAR 4 32 0 1
		else
			add 21 1
			SET_EVENT_VAR 4 32 @21 1
		endif
	endif
ENDPROC

