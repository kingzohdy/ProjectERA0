        <RecvBuff>655360 </RecvBuff>
        <MaxIdle>95 </MaxIdle>
        <Backlog>20 </Backlog>
        <MaxNoAuthIdle>3 </MaxNoAuthIdle>
    </Listener>
    <ChatLogDBConnNum>1 </ChatLogDBConnNum>
    <ChatMonLogDBConnInfo type="ChatMonLogDBConnInfo">
        <Name>world_3_10</Name>
        <Url>udp://192.168.0.77:9320</Url>
    </ChatMonLogDBConnInfo>
