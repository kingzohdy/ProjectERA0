<?xml version="1.0" encoding="GBK" standalone="yes" ?>
<chat_mon_serviceconf version="10">
    <SpawnIP>0.0.0.0</SpawnIP>
    <SpawnPort>8010 </SpawnPort>
    <SpawnNum>1 </SpawnNum>
    <FcgiWebServerIPList></FcgiWebServerIPList>
    <SoapEncoding>UTF8 </SoapEncoding>
    <ChannelKeyStart>0 </ChannelKeyStart>
    <HeartBeatInterval>10 </HeartBeatInterval>
    <DirtyWordFile>../cfg/dirtyword.txt</DirtyWordFile>
    <DirtyWordMd5File>../cfg/dirtyword.txt.md5</DirtyWordMd5File>
    <RoleDBMetaFile>../cfg/role_db_meta.tdr</RoleDBMetaFile>
    <ServiceDBInfo type="TORMConnectDBInfo">
        <IDX>1 </IDX>
        <MajorVersion>0 </MajorVersion>
        <MinVersion>0 </MinVersion>
        <DBMSName>mysql</DBMSName>
        <DBMSConnectionInfo>localhost</DBMSConnectionInfo>
