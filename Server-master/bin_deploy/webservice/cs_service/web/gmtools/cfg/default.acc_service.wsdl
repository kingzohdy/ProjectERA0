mate name:AccPasswdRes
	<struct name="UpdatePasswd" dependonstruct="Account"  version="120" >
		<entry name="Uin" type="int" />
		<entry name="PasswdHash" type="tinyint" count="16" desc="用户密码的二次hash:md5(md5(pwd))" />
	</struct>

	<macro name="MAX_ACC_PASSWD_NUM" value="16" desc="帐号密码返回的数量" />

	<struct name="AccPasswdRes" desc="帐号密码获取返回" version="1674">
		<entry name="Num" type="int" desc="数量"   />
		<entry name="RegionID" type="int" count="MAX_ACC_PASSWD_NUM" refer="Num" desc="区ID"   />
		<entry name="AccPasswd" type="UpdatePasswd" count="MAX_ACC_PASSWD_NUM" refer="Num" desc="帐号密码信息" />
	</struct>

-->
					<s:element minOccurs="0" maxOccurs="1" name="StrXML" type="s:string"/>
				</s:sequence>
			</s:complexType>
