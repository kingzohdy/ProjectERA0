			<!--网站注册成功后向大区数据库插入帐号-->
			<s:complexType name="InsertAccountReq">
				<s:sequence>
					<!--区ID|0表示acc_service配置的所有区-->
					<s:element minOccurs="0" maxOccurs="1" name="RegionID" type="s:int"/>
					<!--帐号名-->
					<s:element minOccurs="0" maxOccurs="1" name="AccName" type="s:string"/>
					<!--按游戏算法加密后的密码|Hex格式-->
					<s:element minOccurs="0" maxOccurs="1" name="PasswordHex" type="s:string"/>
					<!--生日|格式19710101-->
					<s:element minOccurs="0" maxOccurs="1" name="Birthday" type="s:string"/>
				</s:sequence>
			</s:complexType>
			<s:complexType name="InsertAccountRes">
				<s:sequence>
					<!--错误码|0成功 -->
					<s:element minOccurs="1" maxOccurs="1" name="ErrorCode" type="s:int" default="0"/>
					<!--错误信息-->
					<s:element minOccurs="0" maxOccurs="1" name="StrError" type="s0:ErrorResponse"/>
				</s:sequence>
			</s:complexType>
			<!--网站修改密码后调用修改大区密码|调用前请写日志-->
			<s:complexType name="UpdateAccountPasswdReq">
