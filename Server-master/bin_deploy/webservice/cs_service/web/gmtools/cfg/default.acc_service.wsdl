					<!--帐号名-->
					<s:element minOccurs="0" maxOccurs="1" name="AccName" type="s:string" />

					<!--绑定类型| 0解绑 1绑定-->
					<s:element minOccurs="0" maxOccurs="1" name="BindType" type="s:int" />

					<!--行数-->
					<s:element minOccurs="0" maxOccurs="1" name="RowNum" type="s:int" />

					<!--列数-->
					<s:element minOccurs="0" maxOccurs="1" name="ColumnNum" type="s:int" />

					<!--逗号分隔的密保卡内容-->
					<s:element minOccurs="0" maxOccurs="1" name="PasswdCard" type="s:string" />
				</s:sequence>
			</s:complexType>

			<s:complexType name="UpdateAccountPasswdCardRes">
				<s:sequence>
					<!--错误码|0成功 -->
					<s:element minOccurs="1" maxOccurs="1" name="ErrorCode" type="s:int" default="0"/>
					<!--错误信息-->
					<s:element minOccurs="0" maxOccurs="1" name="StrError" type="s0:ErrorResponse"/>

				</s:sequence>
			</s:complexType>
