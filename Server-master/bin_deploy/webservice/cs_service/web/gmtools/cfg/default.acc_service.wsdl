					<s:element minOccurs="0" maxOccurs="1" name="RegionID" type="s:int"/>
					<!--帐号名-->
					<s:element minOccurs="0" maxOccurs="1" name="AccName" type="s:string"/>
					<!--按游戏算法加密后的密码|Hex格式-->
					<s:element minOccurs="0" maxOccurs="1" name="PasswordHex" type="s:string"/>
				</s:sequence>
