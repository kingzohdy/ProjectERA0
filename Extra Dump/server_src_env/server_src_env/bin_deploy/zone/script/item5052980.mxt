;特约网咖礼包

proc 1

	strvar num
	GET_PACK_SPACE 0 num
	
	if num >= 5
		DEC_ITEM_NUM_INPAK 5052980 1 1
		if  @1 = 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5050210 1 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5000002 20 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5001002 20 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5233701 3 1 0
			ADD_BIND_ITEM_NUM_INPAK_NOLIMIT 5041000 3 1 0
		endif
	else
		ADD_SYS_MSG_START
		ADD_SYS_MSG 背包剩余空间不足，请空出5个格子再开启
		SET_SYS_MSG_MODE 4 0 0
		MSG_INFO
	endif
endproc
